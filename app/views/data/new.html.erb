<% content_for :title do %>
  Add Data Source
<% end %>

<div id="ltColumn">
  <% content_for :body_class do %>add_data<% end %>

  <% form_for @source, :url => source_create_path do |f| %>
  <%= show_flash_message %>
  <ul>
    <%= render "form", :form => f %>
    <li>
      <button class="submit" id="data_suggestion_submit" type="submit" value="Submit" name="commit">
        <span>Submit</span>
      </button>
    </li>
  </ul>
  <% end %>
</div>    
